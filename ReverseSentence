class HelloWorld {
    public static void main(String[] args) {
    
        String s="My Name Is Khan";
        System.out.print("Reverse of String is:"+ reverse(s));
        
    }
    public static String reverse(String s){
        int x=s.indexOf(" ");
        
        if(x == -1)
        return s;
        
        return s.substring(x+1) +" "+s.substring(0,x);
    }
}
